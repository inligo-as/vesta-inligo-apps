#!/bin/bash

#source "/usr/local/vesta/plugins/vesta-inligo-apps/func/backups.sh"

server_name="$1"
user="$2"
date="$3"
time="$4"

if [[ ! "$user" || ! "$date" ]]; then
    echo "Invalid arguments"
    echo "Ex: v-inligo-restore-backup <server_name> <user> <date> [time]"
    exit 1
fi

echo "v-inligo-restore-backup: $server_name $user $date $time"